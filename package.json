{
  "name": "boilerplate-fullstack",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack"
  ],
  "scripts": {
    "start": "ts-node server/index.ts",
    "test": "HTTPS=true jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server/index.ts",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.1.1",
    "@auth0/auth0-spa-js": "^2.0.8",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@redux-devtools/extension": "^3.2.3",
    "@types/styled-components": "^5.1.26",
    "aws-jwt-verify": "^4.0.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-oauth2-jwt-bearer": "^1.4.1",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "knex": "^2.3.0",
    "pg": "^8.8.0",
    "react-icons": "^4.9.0",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^6.14.1",
    "sqlite3": "^5.1.6",
    "styled-components": "^5.3.11",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "@devacademy/eslint-config": "^1.8.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.2.6",
    "@types/superagent": "^4.1.17",
    "@types/supertest": "^2.0.12",
    "@types/testing-library__jest-dom": "^5.14.6",
    "babel-loader": "^9.1.3",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^1.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.25.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^25.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "nodemon": "^1.14.9",
    "npm-run-all": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.4",
    "supertest": "^6.2.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "browserslist": "> 2%, not dead"
}
